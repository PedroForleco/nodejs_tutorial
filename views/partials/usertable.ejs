<%- console.log(JSON.stringify(dados)); %>
<%- console.log(JSON.stringify(req.session.username)); %>

<div class="table-container">
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Senha</th>
      </tr>
    </thead>
    <tbody>
      <% for (let i = 0; i < dados.length; i++) {%>
      <% if (dados [i].username == req.session.username) {%>
      <tr style="background-color: green; color:white;">
        <%} else {%>
      <tr>
        <% } %>
        <td><%= dados[i].id %></td>
        <td><%= dados[i].username %></td>
        <td><%= dados[i].password %></td>
      </tr>
      <% } %>
    </tbody>
    <tfoot>
      <tr>
        <td colspan="3">Quantidade de Registros: <%= dados.length %></td>
      </tr>
    </tfoot>
  </table>
</div>