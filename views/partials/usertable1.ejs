<%- console.log(JSON.stringify(dados)) %>

<div class="table-container">
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Senha</th>
      </tr>
    </thead>
    <tbody>
      <% for (let i = 0; i < dados.length; i++) { %>
        <tr>
          <td><%= dados[i].id %></td>
          <td><%= dados[i].id_username %></td>
          <td><%= dados[i].titulo %></td>
          <td><%= dados[i].conteudo %></td>
          <td><%= dados[i].data_criacao%></td>
        </tr>
      <% } %>
    </tbody>
    <tfoot>
      <tr>
        <td colspan="3">Quantidade de Registros: <%= dados.length %></td>
      </tr>
    </tfoot>
  </table>
</div>
